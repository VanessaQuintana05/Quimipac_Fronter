    function cpf(v) {
        v = String(v).replace(/([^0-9\.]+)/g, '');
        v = String(v).replace(/^[\.]/, '');
        v = String(v).replace(/[\.][\.]/g, '');
        v = String(v).replace(/\.(\d)(\d)(\d)/g, '.$1$2');
        v = String(v).replace(/\.(\d{1,2})\./g, '.$1');
        v = v.toString().split('').reverse().join('').replace(/(\d{3})/g, '$1,');
        v = String(v).split('').reverse().join('').replace(/^[\,]/, '');
        return v;
    }

